        android:id="@+id/dynamic_nav_graph"
        app:moduleName="sampledynamicfeature"
        app:graphResName="dynamic_nav_graph"
        app:graphPackage="com.example.dynamicfeaturesampleapp.sampledynamicfeature"
        />
</navigation>